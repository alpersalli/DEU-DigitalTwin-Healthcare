<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gatewayPairingVM" type="com.alpersapplication.app.modules.gatewaypairing.data.viewmodel.GatewayPairingVM"/>
    </data>
    <FrameLayout android:id="@+id/frameGatewaypairing"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_44pxv"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnuser"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowuser"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_bluegray_50_radius_25"
                        android:id="@+id/btnUser"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_50pxh"
                        android:paddingHorizontal="@dimen/_10pxh"
                        android:paddingVertical="@dimen/_10pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_user" tools:ignore="ContentDescription"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnhello"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.7"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHELLO"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{gatewayPairingVM.gatewayPairingModel.txtHELLO}"
                            style="@style/txtInterregular14_1" tools:text="@string/lbl_hello"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJade"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{gatewayPairingVM.gatewayPairingModel.txtJade}"
                            style="@style/txtInterbold24" tools:text="@string/lbl_jade"/>
                    </LinearLayout>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_bluegray_50_radius_20"
                        android:id="@+id/btnBluetooth"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_40pxh"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_bluetooth" tools:ignore="ContentDescription"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackusername"
                android:layout_height="@dimen/_546pxv"
                android:layout_marginTop="@dimen/_45pxv" android:layout_width="@dimen/_307pxh">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUsername"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_54pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_54pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@{gatewayPairingVM.gatewayPairingModel.txtUsername}"
                    style="@style/txtInterregular14_1" tools:text="@string/lbl_username"/>
                <LinearLayout android:id="@+id/linearRowgatewaypairing"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylebluegray_2">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumngatewaypairing"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_93pxv"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.96"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGatewayPairing"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_71pxh"
                            android:layout_marginStart="@dimen/_71pxh"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{gatewayPairingVM.gatewayPairingModel.txtGatewayPairing}"
                            style="@style/txtIntersemibold14" tools:text="@string/lbl_gateway_pairing"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAvailableDevic"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_61pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{gatewayPairingVM.gatewayPairingModel.txtAvailableDevic}"
                            style="@style/txtInterregular14" tools:text="@string/msg_available_devic"/>
                        <LinearLayout android:id="@+id/linearRowprice"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnprice"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_126pxv"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_weight="0.97"
                                android:layout_width="0dp" android:orientation="vertical">
                                <EditText android:ellipsize="end"
                                    android:hint="@string/lbl_esp32_1"
                                    android:id="@+id/etPrice"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:paddingBottom="@dimen/_13pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_10pxh"
                                    android:singleLine="true"
                                    android:text="@={gatewayPairingVM.gatewayPairingModel.etPriceValue}"
                                    android:textColorHint="@color/bluegray_601"
                                    style="@style/etIntersemibold14" tools:ignore="Autofill"/>
                                <EditText android:ellipsize="end"
                                    android:hint="@string/lbl_esp32_2"
                                    android:id="@+id/etPriceOne"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_11pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:paddingBottom="@dimen/_14pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_10pxh"
                                    android:singleLine="true"
                                    android:text="@={gatewayPairingVM.gatewayPairingModel.etPriceOneValue}"
                                    android:textColorHint="@color/bluegray_601"
                                    style="@style/etIntersemibold14" tools:ignore="Autofill"/>
                                <EditText android:ellipsize="end"
                                    android:hint="@string/lbl_esp32_3"
                                    android:id="@+id/etPriceTwo"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_12pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:paddingBottom="@dimen/_13pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_10pxh"
                                    android:singleLine="true"
                                    android:text="@={gatewayPairingVM.gatewayPairingModel.etPriceTwoValue}"
                                    android:textColorHint="@color/bluegray_601"
                                    style="@style/etIntersemibold14" tools:ignore="Autofill"/>
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearColumnslider"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_4pxh"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_weight="0.03"
                                android:layout_width="0dp"
                                android:orientation="vertical" style="@style/groupStylebluegray_51cornerRadius">
                                <View
                                    android:background="@drawable/rectangle_bg_bluegray_600_radius_1_5"
                                    android:id="@+id/lineSlider"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_139pxv"
                                    android:layout_marginBottom="@dimen/_107pxv"
                                    android:layout_marginEnd="@dimen/_2pxh"
                                    android:layout_marginStart="@dimen/_2pxh"
                                    android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_3pxh"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowforget"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_25pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnForget"
                                android:layout_height="@dimen/_48pxv"
                                android:layout_width="@dimen/_110pxh"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_forget"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_1" tools:text="@string/lbl_forget"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnConnect"
                                android:layout_height="@dimen/_48pxv"
                                android:layout_marginStart="@dimen/_30pxh"
                                android:layout_width="@dimen/_110pxh"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_27pxh"
                                android:paddingStart="@dimen/_26pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_connect"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded" tools:text="@string/lbl_connect"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_522pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                </LinearLayout>
                <View android:background="@color/white_A700"
                    android:id="@+id/lineLineThirty"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_56pxv"
                    android:layout_marginTop="@dimen/_56pxv" android:layout_width="@dimen/_307pxh"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylebluegray_1">
            <LinearLayout android:id="@+id/linearRowstatisticsbuto"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStatisticsbuto"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <ImageView android:id="@+id/imageNotification"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notification"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
