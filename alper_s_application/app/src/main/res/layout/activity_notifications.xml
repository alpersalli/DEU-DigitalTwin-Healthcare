<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsVM" type="com.alpersapplication.app.modules.notifications.data.viewmodel.NotificationsVM"/>
    </data>
    <FrameLayout android:id="@+id/frameNotifications"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_44pxv"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearRowlinetwentysix"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnnotification"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_271pxv"
                android:layout_weight="0.98" android:layout_width="0dp" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout
                        android:id="@+id/linearRownotification"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_bluegray_50_radius_25"
                            android:id="@+id/btnNotification"
                            android:layout_height="@dimen/_50pxh"
                            android:layout_width="@dimen/_50pxh"
                            android:paddingHorizontal="@dimen/_10pxh"
                            android:paddingVertical="@dimen/_10pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_notification" tools:ignore="ContentDescription"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNotifications"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_weight="0.74"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{notificationsVM.notificationsModel.txtNotifications}"
                            style="@style/txtInterbold24" tools:text="@string/lbl_notifications"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <View android:background="@color/bluegray_300"
                    android:id="@+id/lineLineTwentySix"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_60pxv" android:layout_width="@dimen/_292pxh"/>
                <FrameLayout android:id="@+id/frameStackreadornotrea"
                    android:layout_height="@dimen/_31pxv"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_292pxh">
                    <View
                        android:background="@drawable/rectangle_bg_bluegray_600_radius_5"
                        android:id="@+id/viewReadornotrea"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_10pxh"/>
                    <LinearLayout
                        android:id="@+id/linearColumnmessagecounter"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMessageCounter"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{notificationsVM.notificationsModel.txtMessageCounter}"
                            style="@style/txtIntersemibold14" tools:text="@string/lbl_message_1"/>
                        <View android:background="@color/bluegray_300"
                            android:id="@+id/lineLineTwentySeven"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_292pxh"/>
                    </LinearLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMessageCounterOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@{notificationsVM.notificationsModel.txtMessageCounterOne}"
                    style="@style/txtIntersemibold14" tools:text="@string/lbl_message_2"/>
                <View android:background="@color/bluegray_300"
                    android:id="@+id/lineLineTwentyEight"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_292pxh"/>
                <EditText android:ellipsize="end"
                    android:hint="@string/lbl_message_3"
                    android:id="@+id/etMessageCounterTwo"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@={notificationsVM.notificationsModel.etMessageCounterTwoValue}"
                    android:textColorHint="@color/bluegray_601"
                    style="@style/etIntersemibold14" tools:ignore="Autofill"/>
                <EditText android:ellipsize="end"
                    android:hint="@string/lbl_message_4"
                    android:id="@+id/etMessageCounterThree"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@={notificationsVM.notificationsModel.etMessageCounterThreeValue}"
                    android:textColorHint="@color/bluegray_601"
                    style="@style/etIntersemibold14" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMessageCounterFour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@{notificationsVM.notificationsModel.txtMessageCounterFour}"
                    style="@style/txtIntersemibold14" tools:text="@string/lbl_message_5"/>
                <View android:background="@color/bluegray_300"
                    android:id="@+id/lineLineThirtyOne"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_292pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnslider"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_110pxv"
                android:layout_weight="0.02" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylebluegray_51cornerRadius">
                <View
                    android:background="@drawable/rectangle_bg_bluegray_600_radius_1_5"
                    android:id="@+id/lineSlider"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_139pxv"
                    android:layout_marginBottom="@dimen/_373pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_3pxh"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylebluegray_1">
            <LinearLayout android:id="@+id/linearRowstatisticsbutt"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStatisticsbutt"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <ImageView android:id="@+id/imageNotificationOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notification"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
