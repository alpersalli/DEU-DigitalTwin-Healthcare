<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountPersonalInfoVM" type="com.alpersapplication.app.modules.accountpersonalinfo.data.viewmodel.AccountPersonalInfoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAccountperso"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_44pxv"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnuser"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowuser"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_bluegray_50_radius_25"
                        android:id="@+id/btnUser"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_width="@dimen/_50pxh"
                        android:paddingHorizontal="@dimen/_10pxh"
                        android:paddingVertical="@dimen/_10pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_user" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHELLO"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_29pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.47"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{accountPersonalInfoVM.accountPersonalInfoModel.txtHELLO}"
                        style="@style/txtInterregular14_1" tools:text="@string/lbl_hello"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackpersonalinform"
                android:layout_height="@dimen/_546pxv"
                android:layout_marginTop="@dimen/_47pxv" android:layout_width="@dimen/_307pxh">

                <LinearLayout
                    android:id="@+id/linearColumnpersonalinform"
                    style="@style/groupStylebluegray_1"
                    android:layout_width="match_parent"
                    android:layout_height="607dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtPersonalInform"
                        style="@style/txtIntersemibold14_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{accountPersonalInfoVM.accountPersonalInfoModel.txtPersonalInform}"
                        tools:text="@string/msg_personal_inform" />

                    <TextView
                        android:id="@+id/txtAge"
                        style="@style/txtInterregular14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_64pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{accountPersonalInfoVM.accountPersonalInfoModel.txtAge}"
                        tools:text="@string/lbl_age" />

                    <EditText
                        android:id="@+id/etAgetextbox"
                        style="@style/etRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:background="@drawable/rectangle_border_black_900_radius_6"
                        android:ellipsize="end"
                        android:inputType="text"
                        android:singleLine="true"
                        android:text="@={accountPersonalInfoVM.accountPersonalInfoModel.etAgetextboxValue}"
                        tools:ignore="Autofill" />

                    <TextView
                        android:id="@+id/txtGender"
                        style="@style/txtInterregular14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{accountPersonalInfoVM.accountPersonalInfoModel.txtGender}"
                        tools:text="@string/lbl_gender" />

                    <EditText
                        android:id="@+id/etGendertextbox"
                        style="@style/etRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:background="@drawable/rectangle_border_black_900_radius_6"
                        android:ellipsize="end"
                        android:inputType="text"
                        android:singleLine="true"
                        android:text="@={accountPersonalInfoVM.accountPersonalInfoModel.etGendertextboxValue}"
                        tools:ignore="Autofill" />

                    <TextView
                        android:id="@+id/txtHeight"
                        style="@style/txtInterregular14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_26pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{accountPersonalInfoVM.accountPersonalInfoModel.txtHeight}"
                        tools:text="@string/lbl_height" />

                    <EditText
                        android:id="@+id/etHeighttextbox"
                        style="@style/etRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:background="@drawable/rectangle_border_black_900_radius_6"
                        android:ellipsize="end"
                        android:inputType="text"
                        android:singleLine="true"
                        android:text="@={accountPersonalInfoVM.accountPersonalInfoModel.etHeighttextboxValue}"
                        tools:ignore="Autofill" />

                    <Button
                        android:id="@+id/button7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="save" />

                    <Button
                        android:id="@+id/button8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="cancel" />

                </LinearLayout>
                <View android:background="@color/white_A700"
                    android:id="@+id/lineLineThirty"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_56pxv"
                    android:layout_marginTop="@dimen/_56pxv" android:layout_width="@dimen/_307pxh"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylebluegray_51cornerRadius">
            <LinearLayout android:id="@+id/linearRowstatistticsbut"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStatistticsbut"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <ImageView android:id="@+id/imageNotification"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notification"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
